{
  "name": "promptxy",
  "version": "2.0.1",
  "type": "module",
  "description": "PromptXY - HTTP 请求代理网关，支持 AI 服务提供商的请求拦截和修改",
  "author": "ekko.bao <your.email@example.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/BeGild/promptxy.git"
  },
  "bugs": {
    "url": "https://github.com/BeGild/promptxy/issues"
  },
  "homepage": "https://github.com/BeGild/promptxy#readme",
  "keywords": [
    "http-proxy",
    "ai-gateway",
    "claude",
    "openai",
    "request-interception",
    "llm-proxy"
  ],
  "bin": "./dist/cli-entry.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./gateway": {
      "types": "./dist/gateway.d.ts",
      "import": "./dist/gateway.js"
    },
    "./api-server": {
      "types": "./dist/api-server.d.ts",
      "import": "./dist/api-server.js"
    },
    "./rules": {
      "types": "./dist/rules/index.d.ts",
      "import": "./dist/rules/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.js"
    },
    "./config": {
      "types": "./dist/config.d.ts",
      "import": "./dist/config.js"
    },
    "./database": {
      "types": "./dist/database.d.ts",
      "import": "./dist/database.js"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "import": "./dist/http.js"
    },
    "./logger": {
      "types": "./dist/logger.d.ts",
      "import": "./dist/logger.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "tsx --watch src/main.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/main.js",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "vitest run tests/integration/ --config=vitest.integration.config.ts",
    "test:integration:watch": "vitest tests/integration/ --config=vitest.integration.config.ts",
    "test:integration:verbose": "vitest run tests/integration/ --config=vitest.integration.config.ts --reporter=verbose",
    "test:integration:coverage": "vitest run tests/integration/ --config=vitest.integration.config.ts --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "sql.js": "^1.12.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^24.10.4",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "prettier": "^3.7.4",
    "tsx": "^4.20.6",
    "typescript": "^5.9.2",
    "vitest": "^4.0.16"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
